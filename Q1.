public class Break{
  
  public static void main(String[] args){
    
    int dan;
    int times;
    
    for(dan = 2; dan <= 9; dan++) {
      
      for(times = 1; ; times++) {
         if (times > dan){
         break;
         }
         
         else{
          System.out.print(dan + "x" + times + "=" + dan * times);
          System.out.println();
         }
          
          
            
            
      }
    }     
  }
}  
